#!/bin/bash

# EXTRACT_IMG - Binary wrapper for EXTRACT_IMG.py
# Image Analysis Tool with Type Detection and Routing

SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
PYTHON_SCRIPT="$SCRIPT_DIR/EXTRACT_IMG.py"

# Check if Python script exists
if [ ! -f "$PYTHON_SCRIPT" ]; then
    echo "Error: EXTRACT_IMG.py not found at $PYTHON_SCRIPT" >&2
    exit 1
fi

# Execute the Python script with all arguments
exec /usr/bin/python3 "$PYTHON_SCRIPT" "$@" 