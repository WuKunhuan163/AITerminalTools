# BACKGROUND_CMD 默认环境变量配置

# 日志文件目录
BACKGROUND_CMD_LOG_DIR="~/tmp/background_cmd_logs"

# 最大同时运行进程数
BACKGROUND_CMD_MAX_PROCESSES="1000"

# 默认shell类型
BACKGROUND_CMD_DEFAULT_SHELL="zsh"

# 进程清理间隔（秒）
BACKGROUND_CMD_CLEANUP_INTERVAL="60"

# 日志文件保留天数
BACKGROUND_CMD_LOG_RETENTION_DAYS="7"

# 是否启用详细日志
BACKGROUND_CMD_VERBOSE="false"
